CREATE TABLE Lekcja (idLekcja BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1) PRIMARY KEY, rodzaj VARCHAR(20) NOT NULL,
 ilosc_godzin VARCHAR(4) NOT NULL);
 
 CREATE TABLE Uczen (idUczen BIGINT GENERATED BY DEFAULT AS IDENTITY (START WITH 1) PRIMARY KEY, Imie VARCHAR(30) NOT NULL, 
nazw VARCHAR(50) NOT NULL, doswiadczenie VARCHAR(50) NOT NULL, idLekcja BIGINT FOREIGN KEY REFERENCES Lekcja(idLekcja));